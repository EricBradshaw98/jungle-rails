<tr>
  <td>
    <%= user.email %>
  </td>
  <td>
    <h4><%= user.password_digest %></h4>
  </td>
  <td>
    <%= user.admin %>
  </td>
  <td>
    <%= user.created_at %>
  </td>
  <td>
    <%= user.updated_at %>
  </td>
  <td>
    <%= link_to fa_icon('trash'), [:admin, user], class: 'btn btn-sm btn-danger', method: :delete, data: { confirm: "Are you sure?" } %>
  </td>
</tr>